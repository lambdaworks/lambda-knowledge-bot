    "family": "knowle",
    "taskRoleArn": "arn:aws:iam::195175520793:role/ecs-task-role",
    "executionRoleArn": "arn:aws:iam::195175520793:role/ecs-execution-role",
    "networkMode": "host",
    "requiresCompatibilities": [
        "EC2"
